    {
 
        sprite_index = spr_player_right;
        image_speed = 0.25;
        if image_index == 1
        {
        show_debug_message("Sound Goes Here");
        var footstep = choose(snd_footstep_0,snd_footstep_1,snd_footstep_2,snd_footstep_3);
        audio_play_sound(footstep,0,0);
        }
       
    motion_set(0,5);   
    }
    
else if keyboard_check(ord('A')) == true || keyboard_check(vk_left) == true
    {
   
        sprite_index = spr_player_left;
        image_speed = 0.25;
        if image_index == 1
            {
            show_debug_message("Sound Goes Here");
            var footstep = choose(snd_footstep_0,snd_footstep_1,snd_footstep_2,snd_footstep_3);
            audio_play_sound( footstep,0,0);
            }
      
    motion_set(180,5);   
    }

else if keyboard_check(ord('W')) = true || keyboard_check(vk_up) == true
    {
    sprite_index = spr_player;
    image_index = 1;
    image_speed = 0;
    motion_set(0,0);
    }
    
else if keyboard_check(ord('S')) = true || keyboard_check(vk_down) == true   
    {
    sprite_index = spr_player;
    image_index = 0;
    image_speed = 0;    
    motion_set(0,0);
    }  

else  
    {
    if sprite_index == spr_player &amp;&amp; image_index == 1
        {   
        image_index = 1;
        image_speed = 0;
        motion_set(0,0); 
        }
    else 
        {
        image_index = 0;
        image_speed = 0;    
        motion_set(0,0);
        }
    }     
      
  
test = instance_nearest(x,y,obj_interactable_parent)
   
if position_meeting(x,y,test) = true 
    {
    if instance_number(obj_enter) = 0
        {
        with (test)
            {
            instance_create(x,y-64,obj_enter);
        }   }
    }

else if position_meeting(x,y+49,test) = true    
    {
    show_debug_message("ABOVE STAIRS.");
    if instance_number(obj_enter) = 0
        {
        with (test)
            {
            instance_create(x,y-64,obj_enter);
        }   }
    }
    
else
    {
    with (obj_enter)
        {
        instance_destroy();
        }
    }    
    


